@model ICollection<AuthenticationClientData>

@if (Model.Count > 0)
{
    <h2>Вход с помощью</h2>
    foreach (AuthenticationClientData p in Model)
    {
        string redirectTo = ViewBag.RedirectTo;
        @Html.RouteLink(p.AuthenticationClient.ProviderName, QuickUrl.RouteExternalLogin(p.AuthenticationClient.ProviderName, redirectTo))@:&nbsp;&nbsp;
    }
}