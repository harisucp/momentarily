@model ICollection<AuthenticationClientData>

@if (Model.Count > 0)
{
    foreach (AuthenticationClientData p in Model.Reverse())
    {
        string redirectTo = ViewBag.RedirectTo;
        @Html.RouteLink("Log in with " + p.AuthenticationClient.ProviderName, QuickUrl.RouteExternalLogin(p.AuthenticationClient.ProviderName, redirectTo), new Dictionary<string, object>() { { "class", p.AuthenticationClient.ProviderName } })
    }
}