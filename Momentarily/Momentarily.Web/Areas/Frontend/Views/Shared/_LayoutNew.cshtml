<style>

    .betaSignUpBtn {
        max-width: 600px;
        margin: 20px auto;
    }

        .betaSignUpBtn a {
            padding: 8px 15px;
            max-width: 350px;
            width: 100%;
            font-size: 18px;
        }

    .covidMenu {
        width: auto !important;
    }

    li.dropdown.dropNav.covidMenu a {
        padding: 3px 0 !important;
    }

    li.dropdown.dropNav.covidMenu li a, li.dropdown.dropNav.covidMenu.afterLoginCovidMenu li a {
        padding: 15px 20px !important;
    }

    .topHeader .navbar-right .covidMenu .dropdown-menu, .topHeader .navbar-right li.dropdown.dropNav.covidMenu.afterLoginCovidMenu .dropdown-menu {
        margin: 22px 0 0;
    }

        .topHeader .navbar-right .covidMenu .dropdown-menu::before {
            left: auto;
            right: 22px;
        }

    li.dropdown.dropNav.covidMenu.afterLoginCovidMenu a {
        padding: 4px 0 !important;
    }

    #divLoading {
        margin: 0px;
        padding: 0px;
        position: fixed;
        right: 0px;
        top: 0px;
        width: 100%;
        height: 100%;
        background-color: #666666;
        z-index: 999999;
        opacity: .8;
        filter: alpha(opacity=70);
        display: none;
    }

        #divLoading p {
            position: fixed;
            top: 30%;
            left: 45%;
            color: White;
        }

        #divLoading span > img {
            padding-left: 0px;
        }
    /*#divLoading span > img {
        position: absolute;
        top: 50%;
        z-index: 99;
    }*/
    .popupCloseBtn {
        position: absolute;
        right: 10px;
