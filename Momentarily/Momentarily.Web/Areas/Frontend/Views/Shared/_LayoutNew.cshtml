    #cookie-prompt {
        position: fixed;
        width: 100%;
        left: 0;
        z-index: 99999;
        bottom: 0;
        padding: 12px 0;
    }

    .cookiealert {
        background: rgba(0,0,0,0.9);
        color: #fff;
        padding: 5px 0;
        font: 12px/18px Arial, Helvetica, sans-serif;
    }

        .cookiealert span.cookie-title {
            display: block;
            color: #fff;
            font-size: 16px;
            font-weight: bold;
            margin: 0.4em 0 0;
        }

        .cookiealert a {
            color: #f90;
            text-decoration: underline;
        }

            .cookiealert a.cookieBtn {
                background-color: #fff;
                padding: 10px 15px;
                border-radius: 50px;
                text-decoration: none;
                color: #000;
                display: table;
                font-weight: 600;
            }

                .cookiealert a.cookieBtn:hover {
                    background-color: #cf2d1e;
                    padding: 10px 15px;
                    border-radius: 50px;
                    text-decoration: none;
                    color: #fff;
                    display: table;
                }

