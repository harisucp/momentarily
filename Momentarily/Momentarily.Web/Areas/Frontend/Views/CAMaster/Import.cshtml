@model Momentarily.ViewModels.Models.CovidGoodViewModel
@{
    Layout = "~/Areas/Frontend/Views/Shared/_LayoutNew.cshtml";
}



<style>
    .disclaimerSection {
        max-width: 630px;
        margin: 0 auto 30px;
        border: 1px solid #ccc;
        padding: 10px 30px 20px;
    }

        .disclaimerSection h3 {
            border-bottom: 1px solid #ccc;
            padding: 0px 0 20px;
            margin-bottom: 10px;
            margin-top: 10px;
        }

        .disclaimerSection ul {
            list-style: none;
            padding: 0;
        }

            .disclaimerSection ul li {
                position: relative;
                padding: 6px 0 6px 20px;
            }

                .disclaimerSection ul li i {
                    color: #cf2d1e;
                    margin-right: 3px;
                    margin-left: -15px;
                }

                .disclaimerSection ul li span {
                    color: #cf2d1e;
                }

    .checkmarketingmailuser {
        font-size: 14px;
        font-weight: normal;
        font-style: normal;
        font-stretch: normal;
        color: #9b9b9b;
        width: calc(100% - 30px);
        padding: 0 10px;
        display: inline-block;
        text-align: left;
        vertical-align: top;
    }

    .gooddsc {
        margin-top: 20px;
    }

    .readonlytxt {
        background-color: #f1ecec;
        pointer-events: none;
    }

    #GoodDescription {
        background-color: #f1ecec;
    }

    .placeOrderSection {
        padding: 0;
        border-radius: 3px;
        overflow: hidden;
        margin-bottom: 40px;
    }

        .placeOrderSection h1 {
            background-color: #cf2d1e;
            color: #fff;
            padding: 10px 14px;
            margin-top: 0;
        }

        .placeOrderSection input[type="text"], .placeOrderSection input[type="email"], .placeOrderSection textarea {
            width: 100%;
            border: 1px solid #ccc;
            padding: 8px 15px;
            border-radius: 3px;
            resize: none;
        }

    .placeOrderForm {
        padding: 0 20px;
    }

    .placeOrderSection .proImage {
        padding: 12px 12px 12px 0;
        margin-bottom: 10px;
    }

        .placeOrderSection .proImage img {
            border-radius: 6px;
            box-shadow: 4px 4px 10px rgba(0,0,0,0.3);
            max-height: 220px;
            overflow: hidden;
        }

    .placeOrderSection textarea {
        min-height: 100px;
    }

    .placeOrderSection .orderDescription textarea {
        min-height: 40px;
        padding: 4px 15px;
    }

    .placeOrderSection button.btn.btn-green.helpBtn {
        margin-top: 15px;
        margin-bottom: 15px;
    }

    input:focus, textarea:focus {
        outline: none;
        box-shadow: none;
    }

    span.shippingUS {
        color: #cf2d1e;
        font-size: 20px;
        margin: 20px 0 10px;
        display: inline-block;
        width: 100%;
    }

    .noCancellations {
        margin-bottom: 0;
        margin-top: 0;
        color: #cf2d1e;
    }
</style>



    <div class="content helpForm mainClass">
        <div class="container">
            @using (Html.BeginForm("Import", "CAMaster", FormMethod.Post, new { @id = "frmCmimport", enctype = "multipart/form-data" }))
            {
                <div class="row" style="display:none">
                    <div class="col-md-10 col-md-offset-1">
                        <div class="contact-us-page placeOrderSection">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="text-center">
                                        <h1>Import excel file in database</h1>
                                    </div>

                                </div>
                            </div>
                            <div class="placeOrderForm">
                                <div class="row">


                                    <div class="col-xs-12 col-sm-8 col-md-8">
                                        <div class="row">
                                            <div class="col-sm-6">
                                                <p style="color:red">@Html.Raw(ViewBag.error)</p>
                                                <div class="form-group">
                                                    <label>Excel File</label>
                                                    <input type="file" name="FileUpload" />

                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label style="visibility:hidden">abc</label>
                                                    <input type="submit" value="Import" name="Import"id="btnSubmit" class="btn btn-primary" />
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-10 col-md-offset-1">
                        <div class="contact-us-page placeOrderSection">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="text-center">
                                        <h1>Email update on sendy</h1>
                                    </div>

                                </div>
                            </div>
                            <div class="placeOrderForm">
                                <div class="row">


                                    <div class="col-xs-12 col-sm-8 col-md-8">
                                        <div class="row">
                                            <div class="col-sm-6">
                                                @*<p style="color:red">@Html.Raw(ViewBag.errorsendy)</p>*@
                                                <div class="form-group">
                                                    <label>Only 250 emails will be updated in a day </label>
                                                    <input type="submit" value="Process" name="Process" id="btnSubmitSendy" class="btn btn-primary" />
                                                </div>
                                                <b style="color:red">@Html.Raw(ViewBag.errorsendy)</b>
                                            </div>

                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
    <div id="divLoading">
        <p>
            Loading. Please wait...<br />
            <span>
                <img src="~/Content/Images/circleloadinganimationOrignal.gif" alt="image" />
            </span>
        </p>

    </div>



